{
  "fields": {
    "invoice_id": {
      "type": "string",
      "description": "Unique invoice identifier"
    },
    "amount": {
      "type": "number",
      "description": "Total invoice amount"
    },
    "currency": {
      "type": "string",
      "enum": [
        "USD",
        "SGD",
        "EUR"
      ],
      "description": "Currency code"
    },
    "date": {
      "type": "string",
      "format": "date-time",
      "optional": true,
      "description": "Invoice date"
    }
  },
  "confidence": {
    "threshold": 85,
    "failOnLowConfidence": true
  },
  "metadata": {
    "name": "Invoice Extraction Schema",
    "version": "1.0.0",
    "description": "Extracts invoice data with 85% minimum confidence requirement"
  }
}